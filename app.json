{
  "expo": {
    "name": "Sasta Flix",
    "description": "SastaFlix: Watch all movies with NO ADS",
    "scheme": "sastaflix",
    "slug": "sastaflix",
    "privacy": "public",
    "version": "0.0.2",
    "platforms": [
      "android",
      "ios",
      "web"
    ],
    "githubUrl": "https://github.com/calebnance/expo-netflix",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "src/assets/icon.png",
    "splash": {
      "backgroundColor": "#000000",
      "image": "src/assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "versionCode": 2,
      "package": "com.rentide.sastaflix"
    },
    "ios": {
      "buildNumber": "0.0.2",
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a1d632ea-94b1-4474-970a-1dad5da72d2c"
      }
    }
  }
}
